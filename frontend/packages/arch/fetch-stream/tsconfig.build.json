{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@coze-arch/ts-config/tsconfig.node.json",
  "compilerOptions": {
    "types": [],
    "moduleResolution": "Node10",
    "module": "CommonJS",
    "lib": ["ES2022", "DOM"],
    "strictNullChecks": true,
    "noImplicitAny": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "tsBuildInfoFile": "dist/tsconfig.build.tsbuildinfo"
  },
  "include": ["src"]
}
