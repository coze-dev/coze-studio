    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch: server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/backend",
            "cwd": "${workspaceFolder}/bin",
            "output": "${workspaceFolder}/bin/__debug_bin",
            "env": {
                "ES_VERSION": "v8"
            },
        },
        {
            "name": "Launch Test",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "cwd": "${workspaceFolder}/bin",
            "program": "${workspaceFolder}/backend/infra/impl/document/crawl/crawl4ai",
            "output": "${workspaceFolder}/bin/__debug_bin",
            "env": {
                "ES_VERSION": "v8"
            },
            "args": [
                "-test.run",
                "TestCrawl"
            ]
        }
    ]
}